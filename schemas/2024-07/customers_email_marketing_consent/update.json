      "type": "integer"
    },
    "email_address": {
      "type": [
        "string",
        "null"
      ],
      "format": "email"
    },
    "email_marketing_consent": {
      "type": "object",
      "properties": {
        "state": {
          "type": "null"
        },
        "opt_in_level": {
          "type": "null"
        },
        "consent_updated_at": {
          "type": "null"
        }
      },
      "required": [
        "state",
        "opt_in_level",
        "consent_updated_at"
      ]
    }
  },
  "required": [
    "customer_id",
    "email_address",
    "email_marketing_consent"
  ]
}