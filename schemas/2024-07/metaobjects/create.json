    },
    "capabilities": {
      "type": "object",
      "properties": {
        "publishable": {
          "type": "object",
          "properties": {
            "status": {
              "type": "string"
            }
          },
          "required": [
            "status"
          ]
        }
      },
      "required": [
        "publishable"
      ]
    }
  },
  "required": [
    "type",
    "handle",
    "created_at",
    "updated_at",
    "display_name",
    "id",
    "definition_id",
    "fields",
    "created_by_staff_id",
    "created_by_app_id",
