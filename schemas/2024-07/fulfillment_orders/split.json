{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "split_line_items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uri"
          },
          "quantity": {
            "type": "integer"
          }
        },
        "required": [
          "id",
          "quantity"
        ]
      }
    },
    "fulfillment_order": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "status"
      ]
    },
    "remaining_fulfillment_order": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "status"
      ]
    },
    "replacement_fulfillment_order": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "status"
      ]
    }
  },
  "required": [
    "split_line_items",
    "fulfillment_order",
    "remaining_fulfillment_order",
    "replacement_fulfillment_order"
  ]
}