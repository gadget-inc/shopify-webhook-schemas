{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "admin_graphql_api_id": {
      "type": "string",
      "format": "uri"
    },
    "status": {
      "type": "string"
    },
    "decline": {
      "type": "object",
      "properties": {
        "reason": {
          "type": "string"
        },
        "note": {
          "type": "string"
        }
      },
      "required": [
        "reason",
        "note"
      ]
    }
  },
  "required": [
    "id",
    "admin_graphql_api_id",
    "status",
    "decline"
  ]
}