      "type": "integer"
    },
    "cycle_start_at": {
      "type": "string",
      "format": "date-time"
    },
    "cycle_end_at": {
      "type": "string",
      "format": "date-time"
    },
    "cycle_index": {
      "type": "integer"
    },
    "contract_edit": {
      "type": [
        "string",
        "null"
      ]
    },
    "billing_attempt_expected_date": {
      "type": "string",
      "format": "date-time"
    },
    "skipped": {
      "type": "boolean"
    },
    "edited": {
      "type": "boolean"
    }
