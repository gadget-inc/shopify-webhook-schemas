{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "happened_at": {
      "format": "date-time",
      "type": "string"
    },
    "id": {
      "format": "uri",
      "type": "string"
    },
    "items_added": {
      "items": {
        "properties": {
          "id": {
            "format": "uri",
            "type": "string"
          },
          "quantity": {
            "type": "integer"
          }
        },
        "required": [
          "id",
          "quantity"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": [
    "happened_at",
    "id",
    "items_added"
  ],
  "type": "object"
}