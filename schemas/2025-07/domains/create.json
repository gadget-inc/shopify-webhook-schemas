{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "host": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "localization": {
      "properties": {
        "alternate_locales": {
          "items": false,
          "type": "array"
        },
        "country": {
          "type": "null"
        },
        "default_locale": {
          "type": "string"
        }
      },
      "required": [
        "alternate_locales",
        "country",
        "default_locale"
      ],
      "type": [
        "null",
        "object"
      ]
    },
    "ssl_enabled": {
      "type": "boolean"
    }
  },
  "required": [
    "host",
    "id",
    "localization",
    "ssl_enabled"
  ],
  "type": "object"
}