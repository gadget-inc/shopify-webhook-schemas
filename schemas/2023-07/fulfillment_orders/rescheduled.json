{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "fulfillment_order": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        },
        "status": {
          "type": "string"
        },
        "fulfill_at": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "id",
        "status",
        "fulfill_at"
      ]
    }
  },
  "required": [
    "fulfillment_order"
  ]
}