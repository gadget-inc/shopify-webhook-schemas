  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "node --import tsx --test test/*.test.ts",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:prettier": "prettier --check \"{spec,src}/**/*.{js,ts}\"",
    "lint:eslint": "eslint --quiet --ext ts,tsx spec src",
