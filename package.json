    "build": "rm -rf dist && tsc",
    "watch": "rm -rf dist && tsc --watch --preserveWatchOutput",
    "prepublishOnly": "pnpm run build",
    "prerelease": "gitpkg publish",
    "clean": "rm -rf *.0x *-v8.log",
    "x": "tsx"
  },
